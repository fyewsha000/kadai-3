<h1>Users</h1>
<table>
	<thead>
		<th>image</th>
		<th>name</th>
		<th colspan = "3"></th>
	</thead>
	<tbody>
	  <% @users.each do |user| %>
	  <tr>
	    <td><%= attachment_image_tag user, :profile_image,format: 'jpeg', size: "50x50" %></td>
	    <td><%= user.name %></td>
	    <td><%= link_to 'Show', user_path(user) %></td>
	  </tr>
	  <% end %>
    </tbody>
</table>